    <td><% medication.currently_Taking %></td>
    <td><% medication.name_Of_Medication %></td>
    <td><% medication.dosage %></td>
    <td><% medication.frequency %></td>
    <td><% medication.prescribing_Physician %></td>
    <td><% medication.special_Instructions %></td>
    <td><% medication.pharmacy %></td>
    <td><% medication.medication_Photo %></td>
    <td><%= link_to 'Show', medication %></td>
    <td><%= link_to 'Edit', edit_medication_path(medication) %></td>
    <td><%= link_to 'Destroy', medication, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
